{
  "name": "quickeats-react",
  "description": "Food Delivery Site",
  "license": "MIT",
  "author": {
    "email": "coreyburns@outlook.com",
    "name": "Corey Burns",
    "url": "https://coreyburns.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/burnsco/quickeats-react.git"
  },
  "version": "0.9.0",
  "scripts": {
    "dev": "next dev -p 3003",
    "emulators": "firebase emulators:start",
    "start": "next start -p 3003",
    "build": "next build",
    "build-export": "next build && next export",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "pnpm run type-check",
    "gen-env": "gen-env-types .env.local -o env.d.ts -e .",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "next-unused": {
    "alias": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/components/hooks/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    },
    "debug": true,
    "include": [
      "hooks",
      "layout",
      "components",
      "configs",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/theme-tools": "2.0.18",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/react": "^14.0.0",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.10.1",
    "formik": "^2.4.3",
    "framer-motion": "^10.16.3",
    "gen-env-types": "^1.3.4",
    "isomorphic-unfetch": "^4.0.2",
    "keen-slider": "^6.8.6",
    "next": "13.4.4",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-intersection-observer": "^9.5.2",
    "react-test-renderer": "^18.2.0",
    "swr": "^2.2.2",
    "typescript": "^5.2.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.22.15",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.6.4",
    "babel-plugin-chakra-ui": "1.0.0-next.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react": "1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-next-dynamic": "^1.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.3.0",
    "next-unused": "^0.0.6",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1"
  }
}
