{
  "name": "quickeats-react",
  "description": "Food Delivery Site",
  "license": "MIT",
  "author": {
    "email": "coreyburns@outlook.com",
    "name": "Corey Burns",
    "url": "https://coreyburns.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/burnsco/quickeats-react.git"
  },
  "version": "0.9.0",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "build-export": "next build && next export",
    "postinstall": "husky install",
    "gen-env": "gen-env-types .env.local -o env.d.ts -e .",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "next-unused": {
    "alias": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/components/hooks/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    },
    "debug": true,
    "include": [
      "hooks",
      "layout",
      "components",
      "configs",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@chakra-ui/theme-tools": "1.1.7",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "^17.0.9",
    "faker": "^5.5.3",
    "firebase": "^8.6.5",
    "firebase-admin": "^9.9.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "gen-env-types": "^1.3.0",
    "isomorphic-unfetch": "^3.1.0",
    "keen-slider": "^5.4.1",
    "next": "^11.1.0",
    "nookies": "^2.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.32.0",
    "react-test-renderer": "^17.0.2",
    "react-ticker": "^1.2.2",
    "swr": "^0.5.6",
    "typescript": "^4.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/react-dom": "^17.0.6",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-chakra-ui": "^1.0.0-next.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-next-dynamic": "^1.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "next-unused": "^0.0.6",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.2"
  }
}
