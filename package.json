{
  "name": "quickeats-react",
  "description": "Food Delivery Site",
  "license": "MIT",
  "author": {
    "email": "coreyburns@outlook.com",
    "name": "Corey Burns",
    "url": "https://coreyburns.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/burnsco/quickeats-react.git"
  },
  "version": "0.9.0",
  "scripts": {
    "dev": "next dev -p 3003",
    "emulators": "firebase emulators:start",
    "start": "next start -p 3003",
    "build": "next build",
    "build-export": "next build && next export",
    "postinstall": "husky install",
    "gen-env": "gen-env-types .env.local -o env.d.ts -e .",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "next-unused": {
    "alias": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/components/hooks/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    },
    "debug": true,
    "include": [
      "hooks",
      "layout",
      "components",
      "configs",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.6",
    "@chakra-ui/theme-tools": "2.0.12",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "firebase": "^8.9.1",
    "firebase-admin": "^10.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.1",
    "gen-env-types": "^1.3.4",
    "isomorphic-unfetch": "^3.1.0",
    "keen-slider": "^6.8.3",
    "next": "12.3.1",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-intersection-observer": "^9.4.0",
    "react-test-renderer": "^18.2.0",
    "swr": "^1.3.0",
    "typescript": "^4.8.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.22",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@types/react-dom": "^18.0.7",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-jest": "^29.2.2",
    "babel-plugin-chakra-ui": "^1.0.0-next.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-next-dynamic": "^1.0.1",
    "jest-watch-typeahead": "^2.2.0",
    "lint-staged": "^13.0.3",
    "next-unused": "^0.0.6",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3"
  }
}
